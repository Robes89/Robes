Function Fa050Agend()

    LOCAL aArea      := GetArea()
    LOCAL dDataAgend := CTOD("")

    LOCAL nOpca      := 0
    LOCAL cTitulo	 := STR0170		//"Alteração no agendamento."
    LOCAL cSequencia := "000000"
